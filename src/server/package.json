    "busboy": "^1.6.0",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "gen-env-types": "1.3.4",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.2",
    "pino": "^8.8.0",
    "prisma": "^4.1.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",

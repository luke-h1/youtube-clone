{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsnd --exit-child --transpile-only src/main.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node build/main.js",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:format": "prisma format",
    "db:studio": "prisma studio",
    "db:migrate-new": "prisma migrate dev --name <NAME>",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "postinstall": "pnpm db:generate"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
